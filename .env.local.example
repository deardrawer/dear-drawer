# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Anthropic API (for AI features - optional if using OpenRouter)
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenRouter API (for AI content generation with DeepSeek)
# Get your key at https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key

# Kakao OAuth
# 카카오 개발자 콘솔에서 앱 생성 후 발급받은 REST API 키
NEXT_PUBLIC_KAKAO_CLIENT_ID=your_kakao_rest_api_key
# 카카오 개발자 콘솔에 등록한 Redirect URI (아래 값과 동일하게 설정)
NEXT_PUBLIC_KAKAO_REDIRECT_URI=http://localhost:3000/api/auth/callback/kakao
# (선택) 카카오 Client Secret - 보안 강화 시 사용
KAKAO_CLIENT_SECRET=your_kakao_client_secret

# JWT
JWT_SECRET=your-super-secret-jwt-key-min-32-characters-long

# Admin Dashboard
ADMIN_PASSWORD=your_admin_password
CRON_SECRET=your_cron_secret_for_auto_deletion
